{
    "manifest_version": 3,
    "name": "Tribunal Online",
    "description": "Sinaliza os usuários do Facebook que tem um histórico de renúncia a lógica",
    "version": "0.1",
    "permissions": ["activeTab"],

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": [
                "content.js"
            ]
        }
    ]
}