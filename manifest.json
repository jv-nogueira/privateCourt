{
    "manifest_version": 3,
    "name": "Tribunal Online",
    "description": "Sinaliza usuários que renunciaram a lógica",
    "version": "0.1",
    "permissions": ["activeTab"],
    "icons": { 
        "48": "icons/censorship.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": [
                "content.js"
            ]
        }
    ]
}